            <name/>
            <date_of_birth/>
            <date_of_birth_method/>
            <ephemeral_dob>
              <dob_calendar/>
              <age_label/>
              <age_years/>
              <age_months/>
              <dob_method/>
              <dob_approx/>
              <dob_raw/>
              <dob_iso/>
              <dob_debug/>
            </ephemeral_dob>
            <sex/>
            <phone/>
            <phone_alternate/>
            <role/>
            <role_other/>
            <external_id/>
            <notes/>
            <user_for_contact>

        </input>
        <input ref="/data/chp/ephemeral_dob/age_months">
          <label ref="jr:itext('/data/chp/ephemeral_dob/age_months:label')"/>
        </input>
        <select appearance="columns" ref="/data/chp/ephemeral_dob/dob_method">
          <label ref="jr:itext('/data/chp/ephemeral_dob/dob_method:label')"/>
          <item>
            <label ref="jr:itext('/data/chp/ephemeral_dob/dob_method/approx:label')"/>
            <value>approx</value>
          </item>
        </select>
        <input ref="/data/chp/ephemeral_dob/dob_debug">
          <label ref="jr:itext('/data/chp/ephemeral_dob/dob_debug:label')"/>
        </input>
      </group>
      <select1 appearance="columns" ref="/data/chp/sex">
        <label ref="jr:itext('/data/chp/sex:label')"/>
        <item>
          <label ref="jr:itext('/data/chp/sex/female:label')"/>
          <value>female</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chp/sex/male:label')"/>
          <value>male</value>
        </item>
      </select1>
      <input ref="/data/chp/phone">
        <label ref="jr:itext('/data/chp/phone:label')"/>
      </input>
      <input ref="/data/chp/phone_alternate">
        <label ref="jr:itext('/data/chp/phone_alternate:label')"/>
      </input>
      <select1 ref="/data/chp/role">
        <label ref="jr:itext('/data/chp/role:label')"/>
        <hint ref="jr:itext('/data/chp/role:hint')"/>
        <item>
          <label ref="jr:itext('/data/chp/role/chp:label')"/>
          <value>chp</value>
        </item>
        <item>
          <label ref="jr:itext('/data/chp/role/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/chp/role_other">
        <label ref="jr:itext('/data/chp/role_other:label')"/>
      </input>
      <input ref="/data/chp/external_id">
        <label ref="jr:itext('/data/chp/external_id:label')"/>
      </input>
      <input appearance="multiline" ref="/data/chp/notes">
        <label ref="jr:itext('/data/chp/notes:label')"/>
      </input>
      <group ref="/data/chp/user_for_contact"/>
      <group appearance="hidden" ref="/data/chp/meta"/>
    </group>
  </h:body>
</h:html>

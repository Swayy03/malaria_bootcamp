      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/name" type="string"/>
      <bind nodeset="/data/init/type" type="string"/>
      <bind nodeset="/data/init/type_selector" type="select1" calculate="../type"/>
      <bind nodeset="/data/init/type_label" type="string" calculate="jr:choice-name( /data/init/type_selector ,' /data/init/type_selector ')"/>
      <bind nodeset="/data/chp/parent" type="string"/>
      <bind nodeset="/data/chp/type" type="string"/>
      <bind nodeset="/data/chp/name" type="string" required="true()"/>
      <bind nodeset="/data/chp/date_of_birth" type="string" calculate="../ephemeral_dob/dob_iso" required="true()"/>
      <bind nodeset="/data/chp/date_of_birth_method" type="string" calculate="../ephemeral_dob/dob_method"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_calendar" type="date" relevant="not(selected(../dob_method,'approx'))" required="true()" jr:constraintMsg="jr:itext('/data/chp/ephemeral_dob/dob_calendar:jr:constraintMsg')" constraint=". &lt;= now()"/>
      <bind nodeset="/data/chp/ephemeral_dob/age_label" readonly="true()" type="string" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/chp/ephemeral_dob/age_years" type="int" relevant="selected(../dob_method,'approx')" required="true()" jr:constraintMsg="jr:itext('/data/chp/ephemeral_dob/age_years:jr:constraintMsg')" constraint=". &gt;= 0 and . &lt;= 130"/>
      <bind nodeset="/data/chp/ephemeral_dob/age_months" type="int" relevant="selected(../dob_method,'approx')" jr:constraintMsg="jr:itext('/data/chp/ephemeral_dob/age_months:jr:constraintMsg')" constraint=". &gt;= 0 and . &lt;= 11"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_method" type="select"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_approx" type="string" calculate="add-date(today(), 0- /data/chp/ephemeral_dob/age_years , 0- /data/chp/ephemeral_dob/age_months )"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_raw" type="string" calculate="if(not(selected(  /data/chp/ephemeral_dob/dob_method ,'approx')), 
 /data/chp/ephemeral_dob/dob_calendar ,
 /data/chp/ephemeral_dob/dob_approx )"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_iso" type="string" calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/chp/ephemeral_dob/dob_debug" readonly="true()" type="string" relevant="false()"/>
      <bind nodeset="/data/chp/sex" type="select1" required="true()"/>
      <bind nodeset="/data/chp/phone" type="tel" jr:constraintMsg="jr:itext('/data/chp/phone:jr:constraintMsg')" constraint="true()"/>
      <bind nodeset="/data/chp/phone_alternate" type="tel" jr:constraintMsg="jr:itext('/data/chp/phone_alternate:jr:constraintMsg')" constraint="true()"/>
      <bind nodeset="/data/chp/role" type="select1"/>
      <bind nodeset="/data/chp/role_other" type="string" relevant="selected(  /data/chp/role ,'other')" required="true()"/>
      <bind nodeset="/data/chp/external_id" type="string"/>
      <bind nodeset="/data/chp/notes" type="string"/>
      <bind nodeset="/data/chp/user_for_contact/create" type="string" calculate=" /data/chp/phone  != '' and (selected(  /data/chp/role ,'chp') or selected(  /data/chp/role ,'chp_supervisor'))"/>
      <bind nodeset="/data/chp/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/chp/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/chp/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
